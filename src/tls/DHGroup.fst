val make_ffdhe: (p:string{length (bytes_of_hex p) < 65536}) -> s:string{length (bytes_of_hex s) < 65536} -> Tot params
let make_ffdhe p q =
  {
    dh_p = bytes_of_hex p;
    dh_g = abyte 2z;
    dh_q = Some (bytes_of_hex q);
    safe_prime = true
  }

let ffdhe2048 =
  let p = "" in
  let q = "" in
  assume (length (bytes_of_hex p) < 65536);
  assume (length (bytes_of_hex q) < 65536);
  make_ffdhe p q

let ffdhe3072 =
